# Production Environment Variables
NODE_ENV=production

# Database
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/oshxona?retryWrites=true&w=majority

# Telegram Bot
BOT_TOKEN=your_bot_token_here
WEBHOOK_URL=https://your-backend.onrender.com/webhook

# Frontend URLs
ADMIN_PANEL_URL=https://your-domain.vercel.app/admin
USER_FRONTEND_URL=https://your-domain.vercel.app/webapp
WEBAPP_URL=https://your-domain.vercel.app/webapp

# JWT
JWT_SECRET=your_super_secret_jwt_key_here

# Server
PORT=5000

# Optional: Cloudinary (if using cloud storage)
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret
