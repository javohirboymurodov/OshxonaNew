{
    "version": 2,
    "framework": "vite",
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "installCommand": "npm install",
    "rewrites": [
      { "source": "/(.*)", "destination": "/index.html" }
    ],
    "builds": [{"src": "package.json", "use": "@vercel/static-build"}],
    "routes": [{"src": "/(.*)", "dest": "/index.html"}]
  }
  